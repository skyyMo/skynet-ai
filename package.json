{
  "name": "skynet-ai",
  "version": "1.0.0",
  "description": "SkyNet AI - Meeting transcript to development stories processor",
  "main": "server/index.js",
  "scripts": {
    "start": "node server/index.js",
    "dev": "nodemon server/index.js",
    "build": "cd client && npm install && npm run build && cd .. && cp -r client/build ./build",
    "install-client": "cd client && npm install",
    "heroku-postbuild": "npm run build"
  },
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=6.0.0"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "@notionhq/client": "^2.2.13",
    "openai": "^4.20.0",
    "node-cron": "^3.0.3"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  }
}